.container{
    width: 100%;
    height: 100%;
    position: absolute;
    margin: 0;
    padding: 0;
    background-image: url(https://s1.ax1x.com/2023/04/13/ppx69sg.png);
    background-repeat: no-repeat;
    background-size:cover;
  }
