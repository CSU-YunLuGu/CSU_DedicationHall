/* pages/protocol/protocol.wxss */
.container {
    height: 180vh;
  
    display: flex;
    flex-direction: column;
    align-content: space-between;
    padding: 0;
  }
  
  .protocol {
    background-color: #86a8ef;
    width: 100%;
    height: 100%;
  }
  
  .content {
    width: 70%;
    height: 78%;
    padding: 20px;
  
    background-color: #fff;
    border-radius: 10px;
    margin: auto;
  
  
    display: flex;
    flex-direction: column;
    align-content: center; 
  }
  
  .content-text {
    text-align: center;
    text-indent: 2em;
    color: black;
  }
  
  .check-protocol {
  
    margin: 5% auto auto auto;
  
    background-color: #fff;
    border-radius: 10px;
  
    width: 70%;
    padding: 20px;
  
    height: 1%;
  
    display: flex;
    align-items: center;
    justify-content: space-around;
  }
  
  .check-text {
    color: #86a8efDF;
    font-size: 0.9em;
  }
  
  .check-box {
    width: 30px;
    height: 30px;
  }
  
  .bottom {
    margin-top: -30%;
  }